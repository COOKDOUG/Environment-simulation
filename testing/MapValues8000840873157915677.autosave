static class MapValues
{
  static int Height;
  static int Width;
  static int CellSize;
  static int BreedingCooldown;
  static int NumOfCreatures;
  static int NumOfPlants;
  
  MapValues(int _height, 
    int _width, 
    int cellSize,
    int breedingCD,
    int numOfCreatures,
    int numOfPlants)
  {
    this.Height = _height;
    this.Width = _width;
    this.CellSize = cellSize;
    this.BreedingCooldown = breedingCD;
    this.NumOfCreatures = numOfCreatures;
    this.NumOfPlants = numOfPlants;
  }
}